    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "b && Fa && FGe && (Gd U c),!(c || (b U a)) U !(e U XGd)",
                "[a,c,d,b,e]",
                "[b,a],[e,c,d]",
                "16",
                "10",
                "tmp.txt"
            ]
        }
    ]
}